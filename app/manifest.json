{
	"name": "Avalon Nano",
	"version": "0.7",
	"description": "Chrome Miner for Avalon Nano",
	"manifest_version": 2,
	"minimum_chrome_version": "41.0.2215.0",
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"permissions": [
		"hid",
		{
			"usbDevices": [{
				"vendorId": 10737,
				"productId": 13297
			}]
		}
	],
	"icons": {
		"16": "assets/avalon_16.png",
		"128": "assets/avalon_128.png"
	}
}
